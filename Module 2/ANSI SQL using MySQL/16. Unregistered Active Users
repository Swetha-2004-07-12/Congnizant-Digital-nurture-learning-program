<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <title>Unregistered Active Users</title>
  <style>
    body { font-family: Arial, sans-serif; padding: 20px; background: #f3f4f6; }
    ul { list-style-type: none; padding-left: 0; }
    li { background: #e1f5fe; margin: 5px 0; padding: 10px; border-left: 5px solid #0288d1; }
  </style>
</head>
<body>

  <h2>Users Created in Last 30 Days but Not Registered</h2>
  <ul id="unregistered-users"></ul>

  <script>
    const today = new Date("2025-05-27");

    // Sample users with created dates
    const users = [
      { id: 1, name: "Alice", createdOn: "2025-05-01" },
      { id: 2, name: "Bob", createdOn: "2025-04-10" },
      { id: 3, name: "Charlie", createdOn: "2025-05-10" },
      { id: 4, name: "David", createdOn: "2025-05-20" },
      { id: 5, name: "Eva", createdOn: "2025-03-15" }
    ];

    // Registrations (userId and eventId)
    const registrations = [
      { userId: 1, eventId: 1 },
      { userId: 2, eventId: 2 },
      { userId: 5, eventId: 3 }
    ];

    // Cutoff date 30 days ago
    const cutoffDate = new Date(today);
    cutoffDate.setDate(today.getDate() - 30);

    // Filter users created in last 30 days
    const recentUsers = users.filter(u => new Date(u.createdOn) >= cutoffDate);

    // User IDs who registered
    const registeredUserIds = new Set(registrations.map(r => r.userId));

    // Filter those recent users who have NOT registered
    const unregisteredUsers = recentUsers.filter(u => !registeredUserIds.has(u.id));

    const ul = document.getElementById("unregistered-users");

    if (unregisteredUsers.length === 0) {
      ul.innerHTML = "<li>No unregistered active users found.</li>";
    } else {
      unregisteredUsers.forEach(user => {
        const li = document.createElement("li");
        li.textContent = `${user.name} (Created on: ${new Date(user.createdOn).toDateString()})`;
       
