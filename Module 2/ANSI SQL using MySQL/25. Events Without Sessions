<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="UTF-8" />
<title>Events Without Sessions</title>
<style>
  body { font-family: Arial, sans-serif; padding: 20px; }
  ul { list-style-type: disc; margin-left: 20px; }
</style>
</head>
<body>

<h2>Events Without Sessions</h2>
<ul id="no-sessions-list"></ul>

<script>
const events = [
  { id: 1, name: "Tech Conference" },
  { id: 2, name: "Startup Meetup" },
  { id: 3, name: "Food Festival" }
];

const sessions = [
  { id: 1, eventId: 1 },
  { id: 2, eventId: 1 },
  { id: 3, eventId: 2 }
  // event 3 has no sessions
];

const eventIdsWithSessions = new Set(sessions.map(s => s.eventId));
const eventsWithoutSessions = events.filter(e => !eventIdsWithSessions.has(e.id));

const ul = document.getElementById('no-sessions-list');

if(eventsWithoutSessions.length === 0){
  ul.innerHTML = '<li>All events have sessions scheduled.</li>';
} else {
  eventsWithoutSessions.forEach(e => {
    const li = document.createElement('li');
    li.textContent = e.name;
    ul.appendChild(li);
  });
}
</script>

</body>
</html>
