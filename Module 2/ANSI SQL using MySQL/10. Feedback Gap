<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <title>Feedback Gap</title>
  <style>
    body { font-family: Arial, sans-serif; padding: 20px; background: #fefefe; }
    .event-item { background: #fffbcc; padding: 12px; margin: 8px 0; border-left: 6px solid #fbc02d; }
  </style>
</head>
<body>

  <h2>Events with Registrations but No Feedback</h2>
  <div id="no-feedback-events"></div>

  <script>
    // Events and registrations
    const events = [
      { id: 1, name: "Tech Expo" },
      { id: 2, name: "Startup Summit" },
      { id: 3, name: "Food Carnival" },
      { id: 4, name: "Art Expo" }
    ];

    const registrations = [
      { userId: 1, eventId: 1 },
      { userId: 2, eventId: 2 },
      { userId: 3, eventId: 2 },
      { userId: 4, eventId: 3 }
    ];

    const feedbacks = [
      { userId: 1, eventId: 1, rating: 4, comment: "Great event!" },
      // No feedback for event 2 and 3 despite registrations
      { userId: 5, eventId: 4, rating: 5, comment: "Loved it!" }
    ];

    // Find event IDs with registrations
    const registeredEventIds = new Set(registrations.map(r => r.eventId));
    // Find event IDs with feedback
    const feedbackEventIds = new Set(feedbacks.map(fb => fb.eventId));

    // Events with registrations but no feedback
    const noFeedbackEvents = [...registeredEventIds].filter(id => !feedbackEventIds.has(id));

    const container = document.getElementById("no-feedback-events");
    if (noFeedbackEvents.length === 0) {
      container.innerHTML = "<p>All registered events have feedback.</p>";
    } else {
      noFeedbackEvents.forEach(eventId => {
        const event = events.find(e => e.id === eventId);
        if(event){
          const div = document.createElement("div");
          div.className = "event-item";
          div.textContent = event.name;
          container.appendChild(div);
        }
      });
    }
  </script>

</body>
</html>
