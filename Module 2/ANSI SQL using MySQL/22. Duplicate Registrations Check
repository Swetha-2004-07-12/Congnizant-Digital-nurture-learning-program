<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="UTF-8" />
<title>Duplicate Registrations Check</title>
<style>body{font-family:Arial,sans-serif;padding:20px;} ul{list-style:none;padding:0;} li{margin:6px 0;color:#d32f2f;}</style>
</head>
<body>

<h2>Duplicate Registrations Detected</h2>
<ul id="duplicates"></ul>

<script>
const registrations = [
  { userId: 1, eventId: 1 },
  { userId: 1, eventId: 1 }, // duplicate
  { userId: 2, eventId: 2 },
  { userId: 3, eventId: 2 },
  { userId: 3, eventId: 2 }, // duplicate
  { userId: 3, eventId: 3 }
];

const users = [
  { id: 1, name: "Alice" },
  { id: 2, name: "Bob" },
  { id: 3, name: "Charlie" }
];

const duplicates = [];
const seen = new Set();

registrations.forEach(r => {
  const key = `${r.userId}-${r.eventId}`;
  if(seen.has(key)){
    duplicates.push(r);
  } else {
    seen.add(key);
  }
});

const ul = document.getElementById('duplicates');

if(duplicates.length === 0){
  ul.innerHTML = '<li>No duplicate registrations found.</li>';
} else {
  duplicates.forEach(d => {
    const user = users.find(u => u.id === d.userId);
    const li = document.createElement('li');
    li.textContent = `User ${user ? user.name : d.userId} is registered multiple times for event ID ${d.eventId}`;
    ul.appendChild(li);
  });
}
</script>

</body>
</html>
