<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Peak Session Hours</title>
  <style>
    body { font-family: Arial, sans-serif; padding: 20px; background: #f0f4f8; }
    .event-summary { background: #fff; margin: 8px 0; padding: 12px; border-left: 4px solid #4CAF50; }
  </style>
</head>
<body>

  <h2>Peak Session Hours (10 AM - 12 PM)</h2>
  <div id="peak-sessions"></div>

  <script>
    const events = [
      {
        name: "Tech Expo",
        sessions: ["2025-06-01T09:00", "2025-06-01T10:30", "2025-06-01T11:15", "2025-06-01T14:00"]
      },
      {
        name: "Startup Meet",
        sessions: ["2025-06-02T10:00", "2025-06-02T13:00", "2025-06-02T15:00"]
      },
      {
        name: "Music Fest",
        sessions: ["2025-06-03T08:30", "2025-06-03T12:30"]
      }
    ];

    function countPeakSessions(sessions) {
      return sessions.filter(time => {
        const hour = new Date(time).getHours();
        return hour >= 10 && hour < 12;
      }).length;
    }

    const container = document.getElementById("peak-sessions");
    events.forEach(event => {
      const peakCount = countPeakSessions(event.sessions);
      const div = document.createElement("div");
      div.className = "event-summary";
      div.textContent = `${event.name}: ${peakCount} session(s) between 10 AM - 12 PM`;
      container.appendChild(div);
    });
  </script>

</body>
</html>
